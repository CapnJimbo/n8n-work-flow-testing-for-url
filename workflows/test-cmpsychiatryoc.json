{
  "name": "Shift Scheduler LLM",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "testcmpsychitaryoc",
        "responseMode": "onReceived",
        "options": {}
      },
      "id": "Webhook",
      "name": "Webhook",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [250, 300]
    },
    {
      "parameters": {
        "values": {
          "string": [
            {
              "name": "formattedTable",
              "value": "={{ Object.keys($json).map(k => `| ${k} | ${$json[k]} |`).join('\\n') }}"
            }
          ]
        },
        "options": {}
      },
      "id": "FormatTable",
      "name": "Format Table",
      "type": "n8n-nodes-base.set",
      "typeVersion": 1,
      "position": [500, 300]
    },
    {
      "parameters": {
        "model": "gpt-3.5-turbo",
        "temperature": 0.5,
        "maxTokens": 300,
        "options": {},
        "messages": [
          {
            "role": "user",
            "content": "Create a shift schedule based on the following data:\n{{$node[\"Format Table\"].json[\"formattedTable\"]}}"
          }
        ]
      },
      "id": "ChatGPT",
      "name": "ChatGPT LLM",
      "type": "n8n-nodes-base.openAi",
      "typeVersion": 1,
      "position": [750, 300]
    },
    {
      "parameters": {
        "respondWith": "firstInputJson"
      },
      "id": "Respond",
      "name": "Respond to Webhook",
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1,
      "position": [1000, 300]
    }
  ],
  "connections": {
    "Webhook": {"main": [[{"node": "Format Table", "type": "main", "index": 0}]]},
    "Format Table": {"main": [[{"node": "ChatGPT", "type": "main", "index": 0}]]},
    "ChatGPT": {"main": [[{"node": "Respond", "type": "main", "index": 0}]]}
  },
  "active": true,
  "settings": {}
}
